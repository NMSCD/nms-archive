      <SetContext zwrite="true" colourWrite="true" cullMode="back"  blendMode="replace" depthTest="always" />
      <BindBuffer sampler="gBufferMap"   sourceRT="CLOUDSHADOWS"        bufIndex="0" sRGBRead="0" />

      <BindBuffer sampler="gBuffer1Map"  sourceRT="CLOUDS_HIGH"         bufIndex="0" sRGBRead="0" />
      <BindBuffer sampler="gBuffer2Map"  sourceRT="CLOUDS_HIGH"         bufIndex="0" sRGBRead="0" />
      <BindBuffer sampler="gBuffer3Map"  sourceRT="CLOUDS_HIGH"         bufIndex="0" sRGBRead="0" />
      <BindBuffer sampler="gBuffer4Map"  sourceRT="CLOUDS_HIGH"         bufIndex="0" sRGBRead="0" />
      <BindBuffer sampler="gBuffer5Map"  sourceRT="CLOUDS_HIGH"         bufIndex="0" sRGBRead="0" />
      <BindBuffer sampler="gBuffer6Map"  sourceRT="CLOUDS_HIGH"         bufIndex="0" sRGBRead="0" />
