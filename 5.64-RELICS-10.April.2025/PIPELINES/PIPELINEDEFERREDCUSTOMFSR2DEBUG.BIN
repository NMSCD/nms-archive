<!-- High Dynamic Range (HDR) utf-8Shading Pipeline -->
<Pipeline>
  <Setup>
    <ScaleInfo resScalingDefault="true" />

    <!-- Pipeline Variables -->
    <PipelineVariable id="SSRProbesAvailable" defaultValue="0" />
    <PipelineVariable id="SSRGeometryDrawCount" />

    <!--Shadow Targets-->
    <RenderTarget id="SHADOWBUF"          depthBuf="true"         numColBufs="0"  format="RGBA8"  depthFormat="DEPTH16" scale="1.0" width="5760" height="1920" shadow="true" hasMips="false" numUniformBuffers="3" crossPipeShareId="SHADOW_SHARED" />
    <RenderTarget id="SHADOWBUF_DS"       depthBuf="true"         numColBufs="0"  format="RED16F" depthFormat="DEPTH16" scale="1.0" width="2880" height="960"  shadow="true" hasMips="false" numUniformBuffers="1" esramPageDepthBuf="134" />

    <!--GBuffer Targets-->
    <RenderTarget id="GBUFFER_BIL"        platforms="HHELD"     depthBuf="false" numColBufs="4"  format0="RGBA8" format1="RG8"   format2="RGB10A2" format3="RGBA8" scale="1.0" applyDrs="true" shareTarget0="GBUFFER"  shareBuffer0="0" shareTarget1="GBUFFER" shareBuffer1="1" shareTarget2="GBUFFER" shareBuffer2="2" shareTarget3="GBUFFER" shareBuffer3="3" />
    <RenderTarget id="GBUFFER"            platforms="HHELD"     depthBuf="false" numColBufs="4"  format0="RGBA8" format1="RG8"   format2="RGB10A2" format3="RGBA8" scale="1.0" applyDrs="true" pointSampleColBuf1="true" pointSampleColBuf2="true" pointSampleColBuf3="true" esramPageColBuf0="308" esramPageColBuf1="250" esramPageColBuf2="134" esramPageColBuf3="192" crossPipeShareId="GBUFFER_SHARED" />
    <RenderTarget id="GBUFFER_BIL"        platforms="NEXT,XBOX,PC,PS4"     depthBuf="false" numColBufs="4"  format0="RGBA8" format1="RG16F"  format2="RGB10A2" format3="RGBA8" scale="1.0" applyDrs="true" shareTarget0="GBUFFER"  shareBuffer0="0" shareTarget1="GBUFFER" shareBuffer1="1" shareTarget2="GBUFFER" shareBuffer2="2" shareTarget3="GBUFFER" shareBuffer3="3" />
    <RenderTarget id="GBUFFER"            platforms="NEXT,XBOX,PC,PS4"     depthBuf="false" numColBufs="4"  format0="RGBA8" format1="RG16F"  format2="RGB10A2" format3="RGBA8" scale="1.0" applyDrs="true" pointSampleColBuf1="true" pointSampleColBuf2="true" pointSampleColBuf3="true" esramPageColBuf0="308" esramPageColBuf1="250" esramPageColBuf2="134" esramPageColBuf3="192" crossPipeShareId="GBUFFER_SHARED" />
   
    <!--SSR Targets-->
    <RenderTarget id="SSR_HIT_TRACE"      platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="3"  scale="0.5"     applyDrs="true" format0="R11FG11FB10F"  format1="RG16" format2="RED8" shareTarget0="SSR_HIT_RAD" shareBuffer0="0"  shareTarget1="SSR_HIT_UVS" shareBuffer1="0" shareTarget2="SSR_HIT_PDF" shareBuffer2="0" />
    <RenderTarget id="SSR_HIT_RAD"        platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="1"  scale="0.5"     applyDrs="true" format="R11FG11FB10F"   pointSampleColBuf0="true" />
    <RenderTarget id="SSR_HIT_UVS"        platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="1"  scale="0.5"     applyDrs="true" format="RG16"           pointSampleColBuf0="true" />
    <RenderTarget id="SSR_HIT_PDF"        platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="1"  scale="0.5"     applyDrs="true" format="RED8"           pointSampleColBuf0="true" />
    <RenderTarget id="SSR_REFL_0"         platforms="NEXT|XB1X"   hasMips="true"   autoGenMips="false" depthBuf="false" numColBufs="1"  scale="0.5"     applyDrs="true" format="R11FG11FB10F"   />
    <RenderTarget id="SSR_REFL_1"         platforms="NEXT|XB1X"   hasMips="true"   autoGenMips="false" depthBuf="false" numColBufs="1"  scale="0.5"     applyDrs="true" format="R11FG11FB10F"   />
    <RenderTarget id="SSR_RESOLVE"        platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="1"  scale="1.0"     applyDrs="true" format="R11FG11FB10F"   />
    <RenderTarget id="SSR_TEMPORAL_F"     platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="3"  scale="1.0"     applyDrs="true" format0="R11FG11FB10F"  format1="RED8" format2="RED8" shareTarget0="SSR_RAD_T_F"  shareBuffer0="0" shareTarget1="SSR_VAR_T_F" shareBuffer1="0" shareTarget2="SSR_MOT_T_F" shareBuffer2="0" />
    <RenderTarget id="SSR_TEMPORAL_H"     platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="3"  scale="1.0"     applyDrs="true" format0="R11FG11FB10F"  format1="RED8" format2="RED8" shareTarget0="SSR_RAD_T_H"  shareBuffer0="0" shareTarget1="SSR_VAR_T_F" shareBuffer1="0" shareTarget2="SSR_MOT_T_H" shareBuffer2="0" />
    <RenderTarget id="SSR_RAD_T_H"        platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="1"  scale="1.0"     applyDrs="true" format="R11FG11FB10F"   pointSampleColBuf0="false" />
    <RenderTarget id="SSR_RAD_T_F"        platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="1"  scale="1.0"     applyDrs="true" format="R11FG11FB10F"   pointSampleColBuf0="false" />
    <RenderTarget id="SSR_VAR_T_H"        platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="1"  scale="0.5"     applyDrs="true" format="RED8"           pointSampleColBuf0="false" />
    <RenderTarget id="SSR_VAR_T_F"        platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="1"  scale="1.0"     applyDrs="true" format="RED8"           pointSampleColBuf0="false" />
    <RenderTarget id="SSR_MOT_T_H"        platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="1"  scale="1.0"     applyDrs="true" format="RED8"           pointSampleColBuf0="false" />
    <RenderTarget id="SSR_MOT_T_F"        platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="1"  scale="1.0"     applyDrs="true" format="RED8"           pointSampleColBuf0="false" />
    <!-- SSR Debug Targets    
    <RenderTarget id="SSR_VIS"            platforms="NEXT|XB1X"   hasMips="false"  autoGenMips="false" depthBuf="false" numColBufs="1"  scale="0.5"     format="RED16F"       pointSampleColBuf0="true" />
    -->

    <!--GTAO Targets-->
    <RenderTarget id="GTAO_FRAME_0"       depthBuf="false"        numColBufs="1"  format="RED8"                 scale="0.5" scale3="1.0"  applyDrs="true" scaleGroup="AmbientOcclusion"  allowDcc="false" />
    <RenderTarget id="GTAO_FRAME_1"       depthBuf="false"        numColBufs="1"  format="RED8"                 scale="0.5" scale3="1.0"  applyDrs="true" scaleGroup="AmbientOcclusion"  allowDcc="false" />
    <RenderTarget id="GTAO_HISTORY_0"     depthBuf="false"        numColBufs="2"  format0="RED8" format1="RED8" scale="0.5" scale3="1.0"  applyDrs="true" scaleGroup="AmbientOcclusion"  allowDcc="false" />
    <RenderTarget id="GTAO_HISTORY_1"     depthBuf="false"        numColBufs="2"  format0="RED8" format1="RED8" scale="0.5" scale3="1.0"  applyDrs="true" scaleGroup="AmbientOcclusion"  allowDcc="false" />

    <!--SSS Targets-->
    <RenderTargetGroup id="ScreenSpaceShadows">
    <RenderTarget id="SSS_FINAL"          depthBuf="false"  numColBufs="1"        format="RED8"                           scale="0.5" applyDrs="true"  allowDcc="true" />
    <RenderTarget id="SSS_TEMP"           depthBuf="false"  numColBufs="1"        format="RED8"                           scale="0.5" applyDrs="true"  allowDcc="true" />
    <RenderTarget id="SSS_HISTORY"        depthBuf="false"  numColBufs="1"        format="RED8"                           scale="0.5" applyDrs="true"  allowDcc="true" />
    <RenderTarget id="SPEED_WEIGHT"       depthBuf="false"  numColBufs="1"        format="RED8"                           scale="1.0" applyDrs="true"  allowDcc="true" />
    <RenderTarget id="SPEED_WEIGHT_H"     depthBuf="false"  numColBufs="1"        format="RED8"                           scale="1.0" applyDrs="true"  allowDcc="true" />
    </RenderTargetGroup>
    
    <!--Misc Targets-->
    <RenderTarget id="SUNOUT_0"           depthBuf="false"        numColBufs="2"  format0="R11FG11FB10F" format1="R11FG11FB10F" scale="1.0" applyDrs="true"  shareTarget0="HDRBUF_0"      shareTarget1="HDRBUF_1"        allowDcc0="true" allowDcc1="false"  />
    <RenderTarget id="BLENDED_ABOVE"      depthBuf="false"        numColBufs="2"  format0="R11FG11FB10F" format1="RGBA8"        scale="1.0" applyDrs="true"  shareTarget0="HDRBUF_5"      shareTarget1="BLENDED_PARAMS"  allowDcc0="true" allowDcc1="false" />
    <RenderTarget id="BLENDED_PARAMS"     depthBuf="false"        numColBufs="1"  format0="RGBA8"                               scale="1.0" applyDrs="true"  shareTarget0="REACT_PARAMS"  allowDcc0="false" />
    <RenderTarget id="SCREEN"                                     numColBufs="1"  depthBuf="false"  format="R11FG11FB10F"                                           scale="1.0" applyDrs="true"  shareTarget0="HDRBUF_2"  shareBuffer0="0"  allowDcc="false" />
    <RenderTarget id="FINAL"              platforms="PC|NEXT|XBOX"          depthBuf="false"  numColBufs="2"       format0="R11FG11FB10F"   format1="RED8"  scale="1.0" applyDrs="true"  shareTarget0="HDRBUF_1"  shareBuffer0="0"  allowDcc="NEXT" />
    <RenderTarget id="UPS_OUT_0"          platforms="PC|NEXT|XBOX|PS4"      depthBuf="false"  numColBufs="1"       format="R11FG11FB10F"                    scale="1.0" applyDrs="false" postUpscale="true"   allowDcc="NEXT" esramPageColBuf0="366" />
    <RenderTarget id="UPS_OUT_1"          platforms="PC|NEXT|XBOX|PS4"      depthBuf="false"  numColBufs="1"       format="R11FG11FB10F"                    scale="1.0" applyDrs="false" postUpscale="true"   allowDcc="NEXT" esramPageColBuf0="134" />

    <!--LUT Targets-->
    <RenderTarget id="LUT_MASK"           depthBuf="false"        numColBufs="1"  format="RED8" scale="1.0" applyDrs="true" shareTarget0="RED8_BUF_0" shareBuffer0="0"  allowDcc="false" />

    <!--Volumetrics Targets-->
    <RenderTarget id="VOLUME"             depthBuf="false"        numColBufs="2"  format0="RGBA16F" format1="RED16F" scale="0.5" applyDrs="true" pointSampleColBuf0="true" pointSampleColBuf1="true" shareTarget1="LIGHTSHAFT" shareBuffer0="0" />
    <RenderTarget id="BLUR1"              depthBuf="false"        numColBufs="1"  format="RGBA16F"  scale="0.5" applyDrs="true" pointSampleColBuf0="true"  esramPageColBuf0="134" />
