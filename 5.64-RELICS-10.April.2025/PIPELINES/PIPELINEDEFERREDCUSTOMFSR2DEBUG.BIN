    <RenderTarget id="MOTION_MAXBUF_0"    platforms="HHELD,PS4,!PC"   depthBuf="false" numColBufs="1" format="RGBA8"                    scale="0.125"  applyDrs="true" allowDcc="false"  />
    <RenderTarget id="MOTION_MAXBUF_1"    platforms="HHELD,PS4,!PC"   depthBuf="false" numColBufs="1" format="RGBA8"                    scale="0.125"  applyDrs="true" allowDcc="false"  />
    <RenderTarget id="MOTIONRESOLVE"      platforms="HHELD,PS4,!PC"   depthBuf="false" numColBufs="1" format="R11FG11FB10F"             scale="1.0"    applyDrs="true" />
    <RenderTarget id="MOTION_MAXBUF_H"    platforms="NEXT,XBOX,PC"    depthBuf="false" numColBufs="1" format="RGBA16"                   scaleX="0.125" scaleY="1.0" applyDrs="true" allowDcc="false" />
    <RenderTarget id="MOTION_MAXBUF_0"    platforms="NEXT,XBOX,PC"    depthBuf="false" numColBufs="1" format="RGBA16"                   scale="0.125"  applyDrs="true" allowDcc="false"  />
    <RenderTarget id="MOTION_MAXBUF_1"    platforms="NEXT,XBOX,PC"    depthBuf="false" numColBufs="1" format="RGBA16"                   scale="0.125"  applyDrs="true" allowDcc="false"  />
