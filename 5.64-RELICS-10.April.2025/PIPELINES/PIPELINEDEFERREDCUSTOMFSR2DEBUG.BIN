    <RenderTarget id="LIGHTS_TILEBUF"     depthBuf="false"        numColBufs="1" format="RED32I" applyDrs="true" pointSampleColBuf="true" scaleX="0.5" scaleY="1.0" allowDcc="false" esramPageColBuf0="76" />

