    <RenderTarget id="CLOUDSHADOWS"       depthBuf="false"        numColBufs="1" format="RED8"   applyDrs="true" scale="0.5" allowDcc="false" />
    <RenderTarget id="CLOUD_BLURBUF1"     depthBuf="false"        numColBufs="1" format="RGBA8"  scale="0.0" allowDcc="false" />
    <RenderTarget id="CLOUD_BLURBUF2"     depthBuf="false"        numColBufs="1" format="RGBA8"  scale="0.0" allowDcc="false" />

    <!--Water Targets-->
    <RenderTarget id="WATER"              depthBuf="false"        numColBufs="1" format="RGBA16F"  scale="0.5"   applyDrs="true" esramPageColBuf0="134"  />

    <!--Particles Targets-->
    <RenderTarget id="PARTICLES"          depthBuf="false"        numColBufs="2" format0="R11FG11FB10F" format1="RGBA8" scale="1.0"   applyDrs="true" shareTarget1="PARTICLES_PARAMS"   />
    <RenderTarget id="PARTICLES_H"        depthBuf="false"        numColBufs="2" format0="R11FG11FB10F" format1="RGBA8" scale="0.5"   applyDrs="true" shareTarget1="PARTICLES_PARAMS_H" />
    <RenderTarget id="PARTICLES_PARAMS"   depthBuf="false"        numColBufs="1" format="RGBA8"                         scale="1.0"   applyDrs="true" shareTarget0="REACT_PARAMS" />
    <RenderTarget id="PARTICLES_PARAMS_H" depthBuf="false"        numColBufs="1" format="RGBA8"                         scale="0.5"   applyDrs="true" />

    <!--Tiled Lighting Targets-->
