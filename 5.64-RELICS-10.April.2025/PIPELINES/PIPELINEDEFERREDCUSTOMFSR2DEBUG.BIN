    <RenderTarget id="FSR2_Debug"                         numColBufs="1"  format="RGB10A2"                                            scale="1.0"   applyDrs="false" allowDcc="true"  postUpscale="true" />

    <!--Upscale Targets-->
    <RenderTarget id="UPS_IN_0"           platforms="PC|PS5"      depthBuf="false"  numColBufs="1"       format="R11FG11FB10F"                    scale="1.0" applyDrs="false" postUpscale="true"   allowDcc="false" />
    <RenderTarget id="UPS_OUT_0"          platforms="PC|PS5"      depthBuf="false"  numColBufs="1"       format="R11FG11FB10F"                    scale="1.0" applyDrs="false" postUpscale="true"   allowDcc="false" />
    <RenderTarget id="UPS_OUT_1"          platforms="PC|PS5"      depthBuf="false"  numColBufs="1"       format="R11FG11FB10F"                    scale="1.0" applyDrs="false" postUpscale="true"   allowDcc="false" />
    <RenderTarget id="UPS_OUT_2"          platforms="PC|PS5"      depthBuf="false"  numColBufs="1"       format="RGB10A2"                         scale="1.0" applyDrs="false" postUpscale="true"   allowDcc="false" />
    <RenderTarget id="UPS_OUT_3"          platforms="PC|PS5"      depthBuf="false"  numColBufs="1"       format="R11FG11FB10F"                    scale="1.0" applyDrs="false" postUpscale="true"   allowDcc="false" />

