    <RenderTarget id="FSR2_PreparedInputColorLuma"        numColBufs="1"  format="RED16"                                              scale="1.0"   applyDrs="true"  allowDcc="true"  shareTarget0="FX_COMBINE_OUT" shareBuffer0="1" />
