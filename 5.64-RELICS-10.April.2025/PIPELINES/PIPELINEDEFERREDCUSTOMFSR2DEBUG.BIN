      <DrawQuadMT material="materials/PostProcess.material.mbin" context="STAA_LOW" />
      <UnbindBuffers />
      <EndTarget flushCB="true" flushDB="false" />
    </Stage> 

    <Stage id="TAA_APPLY_TEST" enabled="false">
